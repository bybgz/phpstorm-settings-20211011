<application>
  <component name="CodeInsightSettings">
    <option name="HIGHLIGHT_SCOPE" value="true" />
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="ERROR_STRIPE_MARK_MIN_HEIGHT" value="1" />
    <option name="SHOW_METHOD_SEPARATORS" value="true" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="12" />
    <option name="FONT_FAMILY" value="Monaco" />
    <option name="FONT_REGULAR_SUB_FAMILY" value="Regular" />
    <option name="FONT_BOLD_SUB_FAMILY" value="Regular" />
    <option name="LINE_SPACING" value="1.4" />
    <option name="SECONDARY_FONT_FAMILY" value="Microsoft YaHei UI" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="PHP" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR:CONSOLE" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*" />
    <option name="ARE_GUTTER_ICONS_SHOWN" value="false" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="ENABLE_RENDERED_DOC" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value="parameter.hints.old" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssInlineColorPreview" value="true" />
  </component>
</application>